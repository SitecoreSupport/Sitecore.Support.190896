<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/" xmlns:set="http://www.sitecore.net/xmlconfig/set/" xmlns:role="http://www.sitecore.net/xmlconfig/role/">
  <sitecore role:require="Standalone OR ContentManagement">
    <processors>
      <uiDeleteItems>
        <processor mode="on" patch:instead="*[@method='CheckTemplateLinks']" type="Sitecore.Support.Shell.Framework.Pipelines.DeleteItems,Sitecore.Support.190896" method="CheckTemplateLinks"/>
        <processor mode="on" patch:instead="*[@method='CheckLinks']" type="Sitecore.Support.Shell.Framework.Pipelines.DeleteItems,Sitecore.Support.190896" method="CheckLinks"/>
      </uiDeleteItems>
    </processors>
  </sitecore>
</configuration>